{
  "authors": [ "Allan Hardy" ],

  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "debugType": "full",
    "additionalArguments": [ "/ruleset:../../AppMetrics.ruleset", "/additionalfile:../../stylecop.json" ]
  },
  "copyright": "Allan Hardy 2016",

  "dependencies": {
    "App.Metrics": "1.0.0-rc1",
    "Newtonsoft.Json": "9.0.1",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "description": "App Metrics Formattor, formatting metrics data to JSON using Json.NET",

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    },
    "net452": {
      "buildOptions": {
        "define": [
          "CLASSIC"
        ]
      },
      "dependencies": {

      },
      "frameworkAssemblies": {
      }

    }
  },

  "packOptions": {
    "tags": [ "Metrics", "Monitoring", "Instrumentation", "JSON" ],
    "projectUrl": "https://alhardy.github.io/app-metrics-docs/getting-started/reporting/index.html#text-file-reporter",
    "licenseUrl": "https://github.com/alhardy/AppMetrics/blob/master/LICENSE",
    "iconUrl": "https://alhardy.github.io/app-metrics-docs/logo.png",
    "repository": {
      "type": "git",
      "url": "git://github.com/alhardy/AppMetrics"
    }
  },
  "title": "App.Metrics.Formatters.Json",
  "version": "1.0.0-rc1"
}
